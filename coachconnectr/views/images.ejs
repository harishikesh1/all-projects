<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Image List</title>
</head>
<body>
    <h1>Image List</h1>
    <% images.forEach(image => { %>
        <% if (image.UserImg) { %>
            <img src="<%= image.UserImg %>" alt="User Image" style="width: 200px;">
        <% } %>
    <% }); %>
</body>
</html>
